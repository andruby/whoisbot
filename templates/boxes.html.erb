<h3>Checking <%= tlds.count %> tlds for base domain <%= base_domain %></h3>
<div>
  <% tlds.each do |tld| %>
    <div class="box box-tld-<%= tld.gsub('.','_') %> <%= ' ignore' if ignore_tlds.include?(tld)%>">
      <a href="<%= ref_link(base_domain, tld) %>"><%= base_domain + "." + tld %></a>
    </div>
  <% end %>
</div>
